{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://cdnjs.cloudflare.com/ajax/libs/socket.io/1.7.2/socket.io.min.js https://code.jquery.com/jquery-3.1.1.min.js; object-src 'self'"
}